<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="org.ocpp.sse.dao.TariffDao">


  <!-- 根据id寻找用户 -->
  <select id="get" parameterType="java.lang.String" resultMap="tariffMap">
		  SELECT * FROM fa_time_of_use_price WHERE charging_station_id = 1
  </select>

	<resultMap type="org.ocpp.sse.entity.Tariff" id="tariffMap">
		<id column="id" property="id"/>
		<result column="charging_station_id" property="chargeStationId"/>
		<result column="starttime" property="startTime"/>
    <result column="stoptime" property="endTime"/>
		<result column="fee" property="fee"/>
		<result column="type" property="periodType" />

  </resultMap>

</mapper>